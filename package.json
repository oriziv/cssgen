{
  "name": "generate-sass-less",
  "version": "1.1.2",
  "description": "",
  "main": "code.js",
  "author": "Ori Ziv",
  "license": "MIT",
  "standard-version": {
    "packageFiles": [
      "package.json"
    ],
    "bumpFiles": [
      "package.json",
      "package-lock.json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write CHANGELOG.md --loglevel silent && git add CHANGELOG.md"
    }
  },
  "scripts": {
    "start": "npx webpack --mode=development --watch",
    "dev": "npx webpack --mode=development --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx webpack --mode=production",
    "prettier": "prettier --write ./**/*",
    "commit": "git-cz",
    "changelog": "standard-version",
    "release": "git push --follow-tags"
  },
  "devDependencies": {
    "@figma/plugin-typings": "1.47.0",
    "@types/file-saver": "^2.0.1",
    "@types/lodash-es": "^4.17.3",
    "@types/prismjs": "^1.16.0",
    "commitizen": "^4.3.0",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "sass": "1.52.3",
    "prettier": "^1.18.2",
    "sass-loader": "^7.2.0",
    "standard-version": "git+https://github.com/conventional-changelog/standard-version.git#feat-bump-files",
    "style-loader": "^1.0.0",
    "ts-loader": "^8.0.0",
    "typescript": "^3.6.4",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "file-saver": "^2.0.2",
    "lodash-es": "^4.17.15",
    "prismjs": "^1.21.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  }
}
