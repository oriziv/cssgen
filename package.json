{
  "name": "generate-sass-less",
  "version": "1.1.1",
  "description": "",
  "main": "code.js",
  "author": "Ori Ziv",
  "license": "MIT",
  "standard-version": {
    "packageFiles": [
      "package.json"
    ],
    "bumpFiles": [
      "package.json",
      "package-lock.json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write CHANGELOG.md --loglevel silent && git add CHANGELOG.md"
    }
  },
  "scripts": {
    "start": "npx webpack --mode=development --watch",
    "dev": "npx webpack --mode=development --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx webpack --mode=production",
    "prettier": "prettier --write ./**/*",
    "commit": "git-cz",
    "changelog": "standard-version",
    "release": "git push --follow-tags"
  },
  "devDependencies": {
    "@figma/plugin-typings": "1.47.0",
    "@types/file-saver": "2.0.5",
    "@types/lodash-es": "^4.17.3",
    "@types/prismjs": "1.26.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "commitizen": "^4.2.4",
    "css-loader": "6.7.1",
    "cz-conventional-changelog": "^3.0.1",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "2.7.0",
    "sass": "1.52.3",
    "sass-loader": "13.0.0",
    "standard-version": "git+https://github.com/conventional-changelog/standard-version.git#feat-bump-files",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.0",
    "typescript": "4.7.3",
    "url-loader": "4.1.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  },
  "dependencies": {
    "file-saver": "^2.0.2",
    "lodash-es": "^4.17.15",
    "prismjs": "^1.21.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
